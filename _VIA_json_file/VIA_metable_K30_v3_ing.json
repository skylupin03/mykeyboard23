{
  "name": "METABLE K30 RGB",
  "vendorId": "0x1C6C",
  "productId": "0xF020",
  "keycodes": ["qmk_lighting"],
  "menus": [
    {
      "label": "Lighting",
      "content": [
        {
          "label": "General",
          "content": [
            {
              "label": "Brightness",
              "type": "range",
              "content": ["id_brightness", 3, 1],
              "options": [0, 255]
            },
            {
              "label": "Effect",
              "type": "dropdown",
              "options": [
                ["All Off", 0],
                ["Solid Color 1", 1],
                ["Alphas/Mods Color 1/2", 2],
                ["Gradient Vertical Color 1/2", 3],
                ["Raindrops Color 1/2", 4],
                ["Cycle All", 5],
                ["Cycle Horizontal", 6],
                ["Cycle Vertical", 7],
                ["Jellybean Raindrops", 8],
                ["Radial All Hues", 9],
                ["Radial Color 1", 10]
              ],
              "content": ["id_effect", 3, 2]
            },
            {
              "label": "Effect Speed",
              "type": "range",
              "options": [0, 3],
              "content": ["id_effect_speed", 3, 3]
            },
            {
              "showIf": "{id_effect} == 1 || {id_effect} == 2 || {id_effect} == 3 || {id_effect} == 4 || {id_effect} == 10",
              "content": [
                {
                  "label": "Color 1",
                  "type": "color",
                  "content": ["id_color_1", 3,4]
                }
              ]
            },
            {
              "showIf": "{id_effect} == 2 || {id_effect} == 3 || {id_effect} == 4",
              "content": [
                {
                  "label": "Color 2",
                  "type": "color",
                  "content": ["id_color_2", 3, 5]
                }
              ]
            }
          ]
        },
        {
          "label": "Advanced",
          "content": [
            {
              "label": "Disable backlight when USB is suspended",
              "type": "toggle",
              "content": ["id_disable_when_usb_suspended", 3, 7]
            },
            {
              "label": "Disable backlight after timeout",
              "type": "range",
              "options": [0, 255],
              "unit": "mins",
              "content": ["id_disable_after_timeout", 3, 8]
            },
            {
              "label": "Caps Lock Indicator",
              "type": "toggle",
              "options": [
                [255, 0],
                [254, 0]
              ],
              "content": ["id_caps_lock_indicator_row_col", 3, 15]
            },
            {
              "label": "Caps Lock Indicator Color",
              "type": "color",
              "showIf": "{id_caps_lock_indicator_row_col.0} == 254",
              "content": ["id_caps_lock_indicator_color", 3, 14]
            },
            {
              "label": "Layer 1 Indicator",
              "type": "toggle",
              "options": [
                [255, 0],
                [254, 0]
              ],
              "content": ["id_layer_1_indicator_row_col", 3, 17]
            },
            {
              "label": "Layer 1 Indicator color",
              "type": "color",
              "showIf": "{id_layer_1_indicator_row_col.0} == 254",
              "content": ["id_layer_1_indicator_color", 3, 16]
            },
            {
              "label": "Layer 2 Indicator",
              "type": "toggle",
              "options": [
                [255, 0],
                [254, 0]
              ],
              "content": ["id_layer_2_indicator_row_col", 3, 19]
            },
            {
              "label": "Layer 2 Indicator Color",
              "type": "color",
              "showIf": "{id_layer_2_indicator_row_col.0} == 254",
